<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="EntityDefinitions" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xs:simpleType name="AnyCsIdentifier">
        <xs:restriction base="xs:token">
            <xs:pattern value="^[A-Z][A-Za-z\d_]*({[A-Z][A-Za-z\d_]*(,[A-Z][A-Za-z\d_]*)*})?$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CsIdentifier">
        <xs:restriction base="AnyCsIdentifier">
            <xs:pattern value="^[A-Z][A-Za-z\d_]*$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CsGenericIdentifier">
        <xs:restriction base="AnyCsIdentifier">
            <xs:pattern value="^[A-Z][A-Za-z\d_]*{[A-Z][A-Za-z\d_]*(,[A-Z][A-Za-z\d_]*)*}$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AnyCsTypeName">
        <xs:restriction base="xs:token">
            <xs:pattern value="^((?=[A-Za-z\d_]+\.)[A-Z][A-Za-z\d_]*\.)*[A-Z][A-Za-z\d_]*({[A-Z][A-Za-z\d_]*(,[A-Z][A-Za-z\d_]*)*})?$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CsTypeName">
        <xs:restriction base="AnyCsTypeName">
            <xs:pattern value="^((?=[A-Za-z\d_]+\.)[A-Z][A-Za-z\d_]*\.)*[A-Z][A-Za-z\d_]*$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CsGenericTypeDefinition">
        <xs:restriction base="AnyCsTypeName">
            <xs:pattern value="^((?=[A-Za-z\d_]+\.)[A-Z][A-Za-z\d_]*\.)*[A-Z][A-Za-z\d_]*{[A-Z][A-Za-z\d_]*(,[A-Z][A-Za-z\d_]*)*}$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CsMemberReference">
        <xs:restriction base="xs:token">
            <xs:pattern value="^((?=[A-Za-z\d_]+[.{])[A-Z][A-Za-z\d_]*\.?)+({[A-Z][A-Za-z\d_]*(,[A-Z][A-Za-z\d_]*)*}\.)?[A-Z][A-Za-z\d_]*" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CsGenericPropertyReference">
        <xs:restriction base="CsMemberReference">
            <xs:pattern value="^((?=[A-Za-z\d_]+\.)[A-Z][A-Za-z\d_]*\.)*[A-Z][A-Za-z\d_]*{[A-Z][A-Za-z\d_]*(,[A-Z][A-Za-z\d_]*)*}\.*[A-Z][A-Za-z\d_]*$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CsEnumFieldReference">
        <xs:restriction base="CsMemberReference">
            <xs:pattern value="^((?=[A-Za-z\d_]+\.)[A-Z][A-Za-z\d_]*\.)+[A-Z][A-Za-z\d_]*$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CsIdentifierOrEmpty">
        <xs:union memberTypes="CsIdentifier">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:length value="0"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>
    <xs:simpleType name="DriveType">
        <xs:restriction base="CsIdentifier">
            <xs:enumeration value="Unknown"/>
            <xs:enumeration value="CDRom"/>
            <xs:enumeration value="Fixed"/>
            <xs:enumeration value="Network"/>
            <xs:enumeration value="NoRootDirectory"/>
            <xs:enumeration value="Ram"/>
            <xs:enumeration value="Removable"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SingleChar">
        <xs:restriction base="xs:string">
            <xs:length value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:group name="DetailsCommentDoc">
        <xs:sequence>
            <xs:element name="remarks" type="CommentDoc" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Remarks xml source code documentation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="seealso" type="CommentDoc" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>See-also xml source code documentation.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="BasicCommentDoc">
        <xs:sequence>
            <xs:element name="summary" type="CommentDoc">
                <xs:annotation>
                    <xs:documentation>Summary xml source code documentation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:group ref="DetailsCommentDoc" />
        </xs:sequence>
    </xs:group>
    <xs:complexType name="CommentDoc" mixed="true">
        <xs:sequence>
            <xs:any processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:group name="PropertyCommentDoc">
        <xs:sequence>
            <xs:element name="summary" type="CommentDoc">
                <xs:annotation>
                    <xs:documentation>Summary xml source code documentation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="value" type="CommentDoc">
                <xs:annotation>
                    <xs:documentation>Value xml source code documentation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:group ref="DetailsCommentDoc" />
        </xs:sequence>
    </xs:group>
    <xs:complexType name="TypeParamCommentDoc">
        <xs:complexContent>
            <xs:extension base="CommentDoc">
                <xs:attribute name="name" type="CsIdentifier">
                    <xs:annotation>
                        <xs:documentation>The name of the generic type parameter name used in the Name of the current entity that refers a generic entity type.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SeeAlsoCommentDoc">
        <xs:complexContent>
            <xs:extension base="CommentDoc">
                <xs:attribute name="cref" type="AnyCsTypeName" />
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="AmbientValueDefinition">
        <xs:attribute name="Value" use="required"/>
    </xs:complexType>
    <xs:group name="AmbientValue">
        <xs:choice>
            <xs:element name="AmbientByte">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="xs:unsignedByte" use="required" />
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AmbientSByte">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="xs:byte" use="required" />
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AmbientShort">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="xs:short" use="required" />
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AmbientUShort">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="xs:unsignedShort" use="required" />
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AmbientInt">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="xs:int" use="required" />
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AmbientUInt">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="xs:unsignedInt" use="required" />
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AmbientLong">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="xs:long" use="required" />
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AmbientULong">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="xs:unsignedLong" use="required" />
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AmbientFloat">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="xs:float" use="required" />
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AmbientDouble">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="xs:double" use="required" />
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AmbientBoolean">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="xs:unsignedByte" use="required" />
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AmbientString">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="xs:unsignedByte" use="required" />
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AmbientEnum">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="AmbientValueDefinition">
                            <xs:attribute name="Value" type="CsTypeName" use="required">
                                <xs:annotation>
                                    <xs:documentation>The full name of an Enum field, matching XPath "/EntityDefinitions/*/EnumTypes/@FullName"</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
        </xs:choice>
    </xs:group>
    <xs:attributeGroup name="MemberAttributes">
        <xs:attribute name="DisplayNameResource" use="optional" type="CsIdentifierOrEmpty">
            <xs:annotation>
                <xs:documentation>The name of the resource for the property's display name.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="DescriptionResource" use="optional" type="CsIdentifier">
            <xs:annotation>
                <xs:documentation>The name of the resource for the property's description.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ResourceType" type="CsTypeName" use="optional">
            <xs:annotation>
                <xs:documentation>The type name of the resource for the property display name and/or description.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:complexType name="MemberDefinitionBase">
        <xs:attribute name="Name" type="CsIdentifier" use="required">
            <xs:annotation>
                <xs:documentation>The name of the field or property.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="FullName" type="CsMemberReference" use="required">
            <xs:annotation>
                <xs:documentation>The full name to reference the member.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="MemberDefinition">
        <xs:complexContent>
            <xs:extension base="MemberDefinitionBase">
                <xs:attributeGroup ref="MemberAttributes" />
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="EnumFieldDefinitionBase">
        <xs:complexContent>
            <xs:restriction base="MemberDefinition">
                <xs:attributeGroup ref="MemberAttributes" />
                <xs:attribute name="FullName" type="CsEnumFieldReference" use="required">
                    <xs:annotation>
                        <xs:documentation>The full name to reference the field.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:group name="EnumFieldElements">
        <xs:sequence>
            <xs:group ref="BasicCommentDoc" />
            <xs:group ref="AmbientValue" minOccurs="0"/>
        </xs:sequence>
    </xs:group>
    <xs:complexType name="EnumFieldDefinition">
        <xs:annotation>
            <xs:documentation>
                Defines an enum field.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="EnumFieldDefinitionBase">
                <xs:group ref="EnumFieldElements" />
                <xs:attribute name="Value" use="required" />
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:attributeGroup name="EnumTypeAttributes">
        <xs:attribute name="Name" type="CsIdentifier" use="required">
            <xs:annotation>
                <xs:documentation>
                    The name of the Enum type.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="IsFlags" type="xs:boolean" use="optional" default="false"/>
    </xs:attributeGroup>
    <xs:complexType name="EnumTypeDefinition">
        <xs:sequence>
            <xs:group ref="BasicCommentDoc" />
            <xs:element name="Field" type="EnumFieldDefinition" minOccurs="2" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attributeGroup ref="EnumTypeAttributes"/>
    </xs:complexType>
    <xs:complexType name="ByteEnumDefinition">
        <xs:annotation>
            <xs:documentation>Defines an enum with a byte base value type.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="EnumTypeDefinition">
                <xs:sequence>
                    <xs:group ref="BasicCommentDoc" />
                    <xs:element name="Field" minOccurs="2" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:restriction base="EnumFieldDefinition">
                                    <xs:group ref="EnumFieldElements" />
                                    <xs:attribute name="Value" type="xs:unsignedByte" use="required"/>
                                </xs:restriction>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attributeGroup ref="EnumTypeAttributes"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SByteEnumDefinition">
        <xs:annotation>
            <xs:documentation>Defines an enum with an sbyte base value type.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="EnumTypeDefinition">
                <xs:sequence>
                    <xs:group ref="BasicCommentDoc" />
                    <xs:element name="Field" minOccurs="2" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:restriction base="EnumFieldDefinition">
                                    <xs:group ref="EnumFieldElements" />
                                    <xs:attribute name="Value" type="xs:byte" use="required"/>
                                </xs:restriction>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ShortEnumDefinition">
        <xs:annotation>
            <xs:documentation>Defines an enum with a short base value type.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="EnumTypeDefinition">
                <xs:sequence>
                    <xs:group ref="BasicCommentDoc" />
                    <xs:element name="Field" minOccurs="2" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:restriction base="EnumFieldDefinition">
                                    <xs:group ref="EnumFieldElements" />
                                    <xs:attribute name="Value" type="xs:short" use="required"/>
                                </xs:restriction>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="UShortEnumDefinition">
        <xs:annotation>
            <xs:documentation>Defines an enum with a ushort base value type.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="EnumTypeDefinition">
                <xs:sequence>
                    <xs:group ref="BasicCommentDoc" />
                    <xs:element name="Field" minOccurs="2" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:restriction base="EnumFieldDefinition">
                                    <xs:group ref="EnumFieldElements" />
                                    <xs:attribute name="Value" type="xs:unsignedShort" use="required"/>
                                </xs:restriction>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="IntEnumDefinition">
        <xs:annotation>
            <xs:documentation>Defines an enum with an int base value type.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="EnumTypeDefinition">
                <xs:sequence>
                    <xs:group ref="BasicCommentDoc" />
                    <xs:element name="Field" minOccurs="2" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:restriction base="EnumFieldDefinition">
                                    <xs:group ref="EnumFieldElements" />
                                    <xs:attribute name="Value" type="xs:int" use="required"/>
                                </xs:restriction>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="UIntEnumDefinition">
        <xs:annotation>
            <xs:documentation>Defines an enum with a uiont base value type.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="EnumTypeDefinition">
                <xs:sequence>
                    <xs:group ref="BasicCommentDoc" />
                    <xs:element name="Field" minOccurs="2" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:restriction base="EnumFieldDefinition">
                                    <xs:group ref="EnumFieldElements" />
                                    <xs:attribute name="Value" type="xs:unsignedInt" use="required"/>
                                </xs:restriction>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="LongEnumDefinition">
        <xs:annotation>
            <xs:documentation>Defines an enum with a long base value type.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="EnumTypeDefinition">
                <xs:sequence>
                    <xs:group ref="BasicCommentDoc" />
                    <xs:element name="Field" minOccurs="2" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:restriction base="EnumFieldDefinition">
                                    <xs:group ref="EnumFieldElements" />
                                    <xs:attribute name="Value" type="xs:long" use="required"/>
                                </xs:restriction>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ULongEnumDefinition">
        <xs:annotation>
            <xs:documentation>Defines an enum with a ulong base value type.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="EnumTypeDefinition">
                <xs:sequence>
                    <xs:group ref="BasicCommentDoc" />
                    <xs:element name="Field" minOccurs="2" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:restriction base="EnumFieldDefinition">
                                    <xs:group ref="EnumFieldElements" />
                                    <xs:attribute name="Value" type="xs:unsignedLong" use="required"/>
                                </xs:restriction>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="NewEntityProperty">
        <xs:complexContent>
            <xs:extension base="MemberDefinitionBase">
                <xs:group ref="PropertyCommentDoc" minOccurs="0" />
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="EntityProperty">
        <xs:complexContent>
            <xs:extension base="MemberDefinition">
                <xs:group ref="PropertyCommentDoc" />
                <xs:attribute name="AllowNull" type="xs:boolean" use="optional" default="false">
                    <xs:annotation>
                        <xs:documentation>Indicates whether to allow null values.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="IsGenericWritable" type="xs:boolean" use="optional" default="false">
                    <xs:annotation>
                        <xs:documentation>Indicates whether the property is read/write on generic interfaces.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="IsUnique" type="xs:boolean" use="optional" default="false">
                    <xs:annotation>
                        <xs:documentation>Indicates whether to add a unique constraint.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="IsIndexed" type="xs:boolean" use="optional" default="false">
                    <xs:annotation>
                        <xs:documentation>Indicates whether the column is indexed.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ColName" type="CsIdentifier" use="optional">
                    <xs:annotation>
                        <xs:documentation>Specifies a column name that is different from the property name.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ByteProperty">
        <xs:complexContent>
            <xs:extension base="EntityProperty">
                <xs:choice minOccurs="0" maxOccurs="1">
                    <xs:element name="Default" type="xs:unsignedByte">
                        <xs:annotation>
                            <xs:documentation>The constant default value.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="DefaultNull">
                        <xs:annotation>
                            <xs:documentation>The default value is null.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType></xs:complexType>
                    </xs:element>
                </xs:choice>
                <xs:attribute name="MinValue" type="xs:unsignedByte" use="optional">
                    <xs:annotation>
                        <xs:documentation>The constant minimum value.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="MaxValue" type="xs:unsignedByte" use="optional">
                    <xs:annotation>
                        <xs:documentation>The constant maximum value.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SByteProperty">
        <xs:complexContent>
            <xs:extension base="EntityProperty">
                <xs:choice minOccurs="0" maxOccurs="1">
                    <xs:element name="Default" type="xs:byte">
                        <xs:annotation>
                            <xs:documentation>The constant default value.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="DefaultNull">
                        <xs:annotation>
                            <xs:documentation>The default value is null.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType></xs:complexType>
                    </xs:element>
                </xs:choice>
                <xs:attribute name="MinValue" type="xs:byte" use="optional">
                    <xs:annotation>
                        <xs:documentation>The constant minimum value.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="MaxValue" type="xs:byte" use="optional">
                    <xs:annotation>
                        <xs:documentation>The constant maximum value.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ByteArrayProperty">
        <xs:complexContent>
            <xs:extension base="EntityProperty">
                <xs:choice minOccurs="0" maxOccurs="1">
                    <xs:element name="Default" type="xs:base64Binary">
                        <xs:annotation>
                            <xs:documentation>The constant default value.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="DefaultNull">
                        <xs:annotation>
