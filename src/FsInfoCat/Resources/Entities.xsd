                </xs:complexType>
            </xs:element>
            <xs:element name="Ambient.Enum">
                <xs:complexType>
                    <xs:attribute name="Value" type="SimpleMemberFullName" use="required" />
                </xs:complexType>
            </xs:element>
        </xs:choice>
    </xs:group>
