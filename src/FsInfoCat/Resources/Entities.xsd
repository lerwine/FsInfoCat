<xs:schema id="EntityDefinitions" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xs:simpleType name="TypeName">
        <xs:restriction base="xs:token">
            <xs:pattern value="^[A-Z][A-Za-z\d_]*({[A-Z][A-Za-z\d_]*(,[A-Z][A-Za-z\d_]*)*})?$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SimpleIdentifier">
        <xs:restriction base="TypeName">
            <xs:pattern value="^[A-Z][A-Za-z\d_]*$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GenericTypeDefinition">
        <xs:restriction base="TypeName">
            <xs:pattern value="^[A-Z][A-Za-z\d_]*{[A-Z][A-Za-z\d_]*(,[A-Z][A-Za-z\d_]*)*}$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CsTypeName">
        <xs:restriction base="xs:token">
            <xs:pattern value="^((?=[A-Za-z\d_]+\.)[A-Z][A-Za-z\d_]*\.)*[A-Z][A-Za-z\d_]*({[A-Z][A-Za-z\d_]*(,[A-Z][A-Za-z\d_]*)*})?$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SimpleCsTypeName">
        <xs:restriction base="CsTypeName">
            <xs:pattern value="^((?=[A-Za-z\d_]+\.)[A-Z][A-Za-z\d_]*\.)*[A-Z][A-Za-z\d_]*$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FullGenericTypeName">
        <xs:restriction base="CsTypeName">
            <xs:pattern value="^((?=[A-Za-z\d_]+\.)[A-Z][A-Za-z\d_]*\.)*[A-Z][A-Za-z\d_]*{[A-Z][A-Za-z\d_]*(,[A-Z][A-Za-z\d_]*)*}$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="MemberReference">
        <xs:restriction base="xs:token">
            <xs:pattern value="^((?=[A-Za-z\d_]+[.{])[A-Z][A-Za-z\d_]*\.?)+({[A-Z][A-Za-z\d_]*(,[A-Z][A-Za-z\d_]*)*}\.)?[A-Z][A-Za-z\d_]*" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SimpleMemberFullName">
        <xs:restriction base="MemberReference">
            <xs:pattern value="^((?=[A-Za-z\d_]+\.)[A-Z][A-Za-z\d_]*\.)+[A-Z][A-Za-z\d_]*$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GenericTypePropertyReference">
        <xs:restriction base="MemberReference">
            <xs:pattern value="^((?=[A-Za-z\d_]+\.)[A-Z][A-Za-z\d_]*\.)*[A-Z][A-Za-z\d_]*{[A-Z][A-Za-z\d_]*(,[A-Z][A-Za-z\d_]*)*}\.*[A-Z][A-Za-z\d_]*$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SimpleIdentifierOrEmpty">
        <xs:union memberTypes="SimpleIdentifier">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:length value="0"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>
    <xs:simpleType name="DriveType">
        <xs:restriction base="SimpleIdentifier">
            <xs:enumeration value="Unknown"/>
            <xs:enumeration value="CDRom"/>
            <xs:enumeration value="Fixed"/>
            <xs:enumeration value="Network"/>
            <xs:enumeration value="NoRootDirectory"/>
            <xs:enumeration value="Ram"/>
            <xs:enumeration value="Removable"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SingleChar">
        <xs:restriction base="xs:string">
            <xs:length value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="CommentDoc" mixed="true">
        <xs:sequence>
            <xs:any processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TypeParamCommentDoc">
        <xs:complexContent>
            <xs:extension base="CommentDoc">
                <xs:attribute name="name" type="SimpleIdentifier">
                    <xs:annotation>
                        <xs:documentation>The name of the generic type parameter name used in the Name of the current entity that refers a generic entity type.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:group name="DetailsCommentDoc">
        <xs:sequence>
            <xs:element name="remarks" type="CommentDoc" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Remarks xml source code documentation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="seealso" type="CommentDoc" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>See-also xml source code documentation.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="SimpleCommentDoc">
        <xs:sequence>
            <xs:element name="summary" type="CommentDoc">
                <xs:annotation>
                    <xs:documentation>Summary xml source code documentation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:group ref="DetailsCommentDoc" />
        </xs:sequence>
    </xs:group>
    <xs:group name="PropertyCommentDoc">
        <xs:sequence>
            <xs:element name="summary" type="CommentDoc">
                <xs:annotation>
                    <xs:documentation>Summary xml source code documentation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="value" type="CommentDoc">
                <xs:annotation>
                    <xs:documentation>Value xml source code documentation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:group ref="DetailsCommentDoc" />
        </xs:sequence>
    </xs:group>
    <xs:group name="AmbientEnumTypes">
        <xs:choice>
            <xs:element name="Ambient.Boolean">
                <xs:complexType>
                    <xs:attribute name="Value" type="xs:boolean" use="required" />
