        <xs:attribute name="DescriptionResource" type="SimpleIdentifier" use="optional" />
        <xs:attribute name="IsGenericWritable" type="xs:boolean" use="optional" default="false" />
        <xs:attribute name="ColName" type="SimpleIdentifier" use="optional" />
