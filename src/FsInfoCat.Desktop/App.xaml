<Application x:Class="FsInfoCat.Desktop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FsInfoCat.Desktop"
             xmlns:vm="clr-namespace:FsInfoCat.Desktop.ViewModel"
             Startup="Application_Startup" Exit="Application_Exit" DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes\Values.xaml"/>
                <ResourceDictionary Source="Themes\Colors.xaml"/>
                <ResourceDictionary Source="Themes\Vector.xaml"/>
                <ResourceDictionary Source="Themes\Styles.xaml"/>
                <ResourceDictionary Source="Themes\ViewModels.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
